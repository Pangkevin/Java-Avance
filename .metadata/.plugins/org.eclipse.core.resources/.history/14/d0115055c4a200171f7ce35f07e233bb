import java.math.*;

public class Sqrt implements Expr {

	private Expr exp1;
	private Expr exp2;
	
	
	
	
	public Multi(Expr exp1, Expr exp2) {
	
		this.exp1 = exp1;
		this.exp2 = exp2;
	}


	@Override
	public double eval() {
		
		System.out.println(this.exp1.eval()+" * " +this.exp2.eval());
		
		return this.exp1.eval() * this.exp2.eval();
				
		
	}

}
